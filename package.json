{
    "name": "with-firebase-auth-serverless",
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev",
        "build": "cross-env NODE_ENV=production next build",
        "start": "cross-env NODE_ENV=production next start",
        "deploy": "next export && firebase deploy --only hosting",
        "dev:all": "cross-env NODE_ENV=development concurrently --kill-others \"yarn dev\" \"cd functions && yarn serve\"",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "lint": "eslint .",
        "postinstall": "cd functions && yarn install",
        "test": "jest .",
        "test:all": "jest ."
    },
    "lint-staged": {
        "*.{json,md}": [
            "prettier --write"
        ],
        "*.{js,jsx}": [
            "prettier --write",
            "eslint"
        ]
    },
    "engines": {
        "node": ">= 10"
    },
    "dependencies": {
        "@onfleet/node-onfleet": "^1.0.5",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-sass": "^1.0.1",
        "axios": "^0.19.2",
        "body-parser": "^1.19.0",
        "cookie-session": "1.4.0",
        "cors": "^2.8.5",
        "dotenv": "8.2.0",
        "firebase": "^7.12.0",
        "firebase-admin": "^8.10.0",
        "firebase-functions": "^3.5.0",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "4.17.15",
        "next": "latest",
        "prop-types": "15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-firebaseui": "4.1.0",
        "react-hook-form": "^5.1.1",
        "react-icons": "^3.9.0",
        "react-is": "^16.13.1",
        "react-text-mask": "^5.4.3",
        "sass": "^1.26.3",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-react": "^7.19.0",
        "husky": "^4.2.1",
        "lint-staged": "^10.0.9",
        "node-sass": "^4.13.1",
        "prettier": "^1.19.1",
        "webpack": "^4.42.0"
    },
    "license": "MIT"
}
