        "body-parser": "^1.19.0",
        "classnames": "^2.2.6",
        "cookie-session": "1.4.0",
        "cors": "^2.8.5",
        "dotenv": "8.2.0",
        "firebase": "^7.12.0",
        "firebase-admin": "^8.10.0",
        "firebase-functions": "^3.5.0",
        "formik": "^2.1.4",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "4.17.15",
        "next": "latest",
        "prop-types": "15.7.2",
